<?xml version='1.0' encoding='utf-8'?>
<widget id="io.attd.qantasapp" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Qantas</name>
    <description>Qantas to the door</description>
    <author email="" href="">Sam Killin &amp; Sam Elliott</author>

    <platform name="ios">
            <icon src="buildAssets/prod-icon/Icon-Small.png" width="29" height="29" />
            <icon src="buildAssets/prod-icon/Icon-Small@2x.png" width="58" height="58" />
            <icon src="buildAssets/prod-icon/Icon-Small@3x.png" width="87" height="87" />
            <icon src="buildAssets/prod-icon/Icon-Small-40.png" width="40" height="40" />
            <icon src="buildAssets/prod-icon/Icon-Small-40@2x.png" width="80" height="80" />
            <icon src="buildAssets/prod-icon/Icon-Small-40@3x.png" width="120" height="120" />
            <icon src="buildAssets/prod-icon/Icon-Small-50.png" width="50" height="50" />
            <icon src="buildAssets/prod-icon/Icon-Small-50@2x.png" width="100" height="100" />
            <icon src="buildAssets/prod-icon/Icon.png" width="57" height="57" />
            <icon src="buildAssets/prod-icon/Icon@2x.png" width="114" height="114" />
            <icon src="buildAssets/prod-icon/Icon-60.png" width="60" height="60" />
            <icon src="buildAssets/prod-icon/Icon-60@2x.png" width="120" height="120" />
            <icon src="buildAssets/prod-icon/Icon-60@3x.png" width="180" height="180" />
            <icon src="buildAssets/prod-icon/Icon-72.png" width="72" height="72" />
            <icon src="buildAssets/prod-icon/Icon-72@2x.png" width="144" height="144" />
            <icon src="buildAssets/prod-icon/Icon-76.png" width="76" height="76" />
            <icon src="buildAssets/prod-icon/Icon-76@2x.png" width="152" height="152" />

            <splash src="buildAssets/splash/Default.png" width="320" height="480" />
            <splash src="buildAssets/splash/Default@2x.png" width="640" height="640" />
            <splash src="buildAssets/splash/Default-568h@2x.png" width="640" height="1136" />
            <splash src="buildAssets/splash/Default-667h@2x.png" width="750" height="1334" />
            <splash src="buildAssets/splash/Default-Portrait-736h@3x.png" width="1242" height="2208" />
            <splash src="buildAssets/splash/Default-Landscape-736h@3x.png" width="2208" height="1242" />
    </platform>

    <platform name="android">
            <icon src="buildAssets/android-icons-prod/xxxhdpi.png" density="xxxhdpi" />
            <icon src="buildAssets/android-icons-prod/xxhdpi.png" density="xxhdpi" />
            <icon src="buildAssets/android-icons-prod/xhdpi.png" density="xhdpi" />
            <icon src="buildAssets/android-icons-prod/hdpi.png" density="hdpi" />
            <icon src="buildAssets/android-icons-prod/mdpi.png" density="mdpi" />
    </platform>

        <preference name="EnableViewportScale" value="true"/>
        <preference name="TopActivityIndicator" value="white"/>
        <preference name="GapBetwenPages" value="2"/>
        <preference name="DisallowOverscroll" value="true"/>
        <preference name="StatusBarStyle" value="blackopaque"/>
        <preference name="target-device" value="handset"/>
        <preference name="StatusBarBackgroundColor" value="#FFFFFF"/>
        <preference name="orientation" value="portrait"/>

        <feature name="http://api.phonegap.com/1.0/battery"/>
        <feature name="http://api.phonegap.com/1.0/camera"/>
        <feature name="http://api.phonegap.com/1.0/contacts"/>
        <feature name="http://api.phonegap.com/1.0/file"/>
        <feature name="http://api.phonegap.com/1.0/geolocation"/>
        <feature name="http://api.phonegap.com/1.0/media"/>
        <feature name="http://api.phonegap.com/1.0/network"/>
        <feature name="http://api.phonegap.com/1.0/notification"/>

    <content src="index.html" />

        <access origin="*" />
</widget>
